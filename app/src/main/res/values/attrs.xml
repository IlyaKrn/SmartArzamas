<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="TableMessageImages">
        <attr name="messageType" format="flags">
            <flag name="system" value="1"/>
            <flag name="my" value="2"/>
            <flag name="not_my" value="3"/>
        </attr>

    </declare-styleable>

    <attr name="color_default_light" format="reference|color"/>
    <attr name="color_default_main" format="reference|color"/>
    <attr name="color_default_dark" format="reference|color"/>

    <attr name="color_red_light" format="reference|color"/>
    <attr name="color_red_main" format="reference|color"/>
    <attr name="color_red_dark" format="reference|color"/>

    <attr name="color_gray_light" format="reference|color"/>
    <attr name="color_gray_main" format="reference|color"/>
    <attr name="color_gray_dark" format="reference|color"/>

    <attr name="color_black_light" format="reference|color"/>
    <attr name="color_black_main" format="reference|color"/>
    <attr name="color_black_dark" format="reference|color"/>

    <attr name="iconColor" format="reference|color"/>

</resources>